// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "out": true,
        "node_modules": true,
        "builds": true,
        ".vscode": true,
        "LICENSE": true,
        "**/__pycache__": true,
        "**/**.pyc": true,
        "*.code-workspace": true,
        "webview-ui/": true, // This folder is added as a seperate workspace folder in vscode-unreal-python.code-workspace
        "test": true
    },

    "search.exclude": {
        "out": true
    },

    // Turn off tsc task auto detection since we have the necessary tasks as npm scripts
    "typescript.tsc.autoDetect": "off"
}